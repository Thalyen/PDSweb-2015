<p id="notice"><%= notice %></p>

<h1>Listing Ocorrencia</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Descricao</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ocorrencia.each do |ocorrencium| %>
      <tr>
        <td><%= ocorrencium.titulo %></td>
        <td><%= ocorrencium.descricao %></td>
        <td><%= ocorrencium.data %></td>
        <td><%= link_to 'Show', ocorrencium %></td>
        <td><%= link_to 'Edit', edit_ocorrencium_path(ocorrencium) %></td>
        <td><%= link_to 'Destroy', ocorrencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ocorrencium', new_ocorrencium_path %>
